<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <children>
      <Button fx:id="botaoCriar" layoutX="113.0" layoutY="221.0" mnemonicParsing="false" onAction="#botaoCriar" text="Criar Perfil" />
      <Button fx:id="botaoDeletar" layoutX="108.0" layoutY="180.0" mnemonicParsing="false" onAction="#botaoDeletar" text="Deletar Perfil" />
      <ComboBox fx:id="listaPerfil" editable="true" layoutX="50.0" layoutY="100.0" prefHeight="25.0" prefWidth="200.0" />
      <Button fx:id="botaoEntrar" layoutX="124.0" layoutY="138.0" mnemonicParsing="false" text="Entrar" />
   </children>
</AnchorPane>
